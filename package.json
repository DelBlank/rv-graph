{
  "name": "rv-graph",
  "version": "0.1.0",
  "main": "dist/es5/vis-graph.js",
  "module": "dist/es6/vis-graph.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "vis",
    "network",
    "graph"
  ],
  "author": {
    "name": "delblank",
    "email": "cpyjss@gmail.com"
  },
  "scripts": {
    "start": "webpack-dev-server --colors",
    "lint": "eslint . --fix --format codeframe",
    "lint-staged": "lint-staged",
    "test": "NODE_ENV=development jest --forceExit",
    "build": "rm -rf dist && npm run build:es5 && npm run build:es6",
    "build:es5": "babel src -d dist/es5 --copy-files",
    "build:es6": "NODE_ENV=production babel src -d dist/es6 --copy-files",
    "precommit": "npm run test && lint-staged"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "react": "^16.3.1",
    "uuid": "^3.3.2",
    "vis": "^4.21.0"
  },
  "devDependencies": {
    "antd": "^3.4.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.11.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.13.4",
    "jest-cli": "^20.0.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lint-staged": "^3.4.0",
    "postcss-loader": "^2.0.9",
    "prettier": "^1.8.1",
    "prop-types": "^15.6.2",
    "react-dom": "^16.3.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.7.1"
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "verbose": true,
    "notify": true,
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "collectCoverage": true,
    "roots": [
      "<rootDir>/__tests__"
    ],
    "modulePaths": [
      "<rootDir>/"
    ],
    "moduleDirectories": [
      "<rootDir>/__mocks__",
      "node_modules"
    ],
    "coverageReporters": [
      "html",
      "text",
      "text-summary"
    ]
  }
}
